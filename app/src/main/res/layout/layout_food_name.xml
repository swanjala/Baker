<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="65dp"
    android:layout_marginBottom="4dp"
    android:elevation="4dp">


    <TextView
        android:id="@+id/tv_food_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/layout_food_name_tv_name_margin_bottom"
        android:layout_marginEnd="@dimen/layout_food_name_tv_name_margin_end"
        android:layout_marginLeft="@dimen/layout_food_name_tv_name_margin_left"
        android:layout_marginRight="@dimen/layout_food_name_tv_name_margin_right"
        android:layout_marginStart="@dimen/layout_food_name_tv_name_margin_start"
        android:layout_marginTop="@dimen/layout_food_name_tv_name_margin_top"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.166" />
</android.support.v7.widget.CardView>